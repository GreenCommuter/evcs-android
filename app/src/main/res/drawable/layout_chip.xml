<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/evcs_gray_100"/>
            <corners android:radius="100dp"/>

        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/evcs_secondary_700"/>
            <corners android:radius="100dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/evcs_white"/>
            <corners android:radius="100dp"/>
            <stroke android:width="@dimen/spacing_tiniest"
                android:color="@color/evcs_gray_600"/>
        </shape>
    </item>
</selector>