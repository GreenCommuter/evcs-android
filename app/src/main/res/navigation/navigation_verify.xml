<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_verify.xml"
    app:startDestination="@id/registerFragmentYourCar">

    <fragment android:id="@+id/placeholder"
        android:name="org.evcs.android.ui.fragment.EmptyFragment">
        <action
            android:id="@+id/action_placeholder_to_planInfoFragment"
            app:destination="@id/registerFragmentYourCar" />
        <action
            android:id="@+id/action_placeholder_to_fragmentValidateOldUser"
            app:destination="@id/fragmentValidateOldUser" />
        <action
            android:id="@+id/action_placeholder_to_changePhoneFragment"
            app:destination="@id/changePhoneFragment" />
    </fragment>

    <fragment
        android:id="@+id/registerFragmentYourCar"
        android:name="org.evcs.android.features.auth.register.RegisterFragmentYourCar"
        android:label="RegisterFragmentYourCar"
        tools:layout="@layout/fragment_register_your_car">
        <action
            android:id="@+id/action_registerFragmentYourCar_to_registerFragmentCellPhone"
            app:destination="@id/registerFragmentCellPhone" />
    </fragment>

    <fragment
        android:id="@+id/fragmentValidateOldUser"
        android:name="org.evcs.android.features.auth.register.FragmentValidateOldUser"
        android:label="RegisterFragmentYourCar"
        tools:layout="@layout/fragment_validate_old_user">
        <action
            android:id="@+id/action_registerFragmentYourCar_to_registerFragmentCellPhone"
            app:destination="@id/fragmentCellPhoneOldUser" />
    </fragment>

    <fragment
        android:id="@+id/registerFragmentCellPhone"
        android:name="org.evcs.android.features.auth.register.RegisterFragmentCellPhone"
        android:label="RegisterFragmentCellPhone"
        tools:layout="@layout/fragment_register_cell_phone">
        <action
            android:id="@+id/action_registerFragmentCellPhone_to_registerFragmentVerify"
            app:destination="@id/registerFragmentVerify" />
    </fragment>
    <fragment
        android:id="@+id/registerFragmentVerify"
        android:name="org.evcs.android.features.auth.register.RegisterFragmentVerify"
        android:label="RegisterFragmentVerify"
        tools:layout="@layout/fragment_register_enter_code">
        <argument
            android:name="previous_number"
            app:argType="string"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/changePhoneFragment"
        android:name="org.evcs.android.features.auth.register.ChangePhoneFragment"
        android:label="ChangePhoneFragment"
        tools:layout="@layout/fragment_register_cell_phone">
        <action
            android:id="@+id/action_changePhoneFragment_to_registerFragmentVerify"
            app:destination="@id/registerFragmentVerify" />
    </fragment>

    <fragment
        android:id="@+id/fragmentCellPhoneOldUser"
        android:name="org.evcs.android.features.auth.register.FragmentCellPhoneOldUser"
        android:label="FragmentCellPhoneOldUser"
        tools:layout="@layout/fragment_register_cell_phone">
        <action
            android:id="@+id/action_fragmentCellPhoneOldUser_to_registerFragmentVerify"
            app:destination="@id/registerFragmentVerify" />
    </fragment>

</navigation>